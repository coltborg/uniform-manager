{
  "name": "uniform-manager",
  "version": "1.0.0",
  "description": "A Nuxt.js project to manage band equipment.",
  "author": "coltborg <coltborg@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "concurrently --kill-others-on-fail \"echo 'Open browser to https://localhost:3334'\" \"local-ssl-proxy --source 3334 --target 3000\"  \"nuxt\"",
